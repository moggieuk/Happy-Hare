# ADDITIONAL FILAMENT DRIVE GEAR STEPPERS FOR TYPE-B MMU's -------------------------------------------------------------
# Note that common parameters are inherited from base stepper_mmu_gear, but can be uniquely specified here too
#
# Filament Drive Gear --------------------------
[tmc2209 stepper_mmu_gear_%]
uart_pin: mmu:MMU_GEAR_UART_%

[stepper_mmu_gear_%]
step_pin: mmu:MMU_GEAR_STEP_%
dir_pin: !mmu:MMU_GEAR_DIR_%
enable_pin: !mmu:MMU_GEAR_ENABLE_%

